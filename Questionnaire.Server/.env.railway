# Backend Environment Variables for Railway
# These should be set in Railway dashboard for the backend service

# Database Configuration (Railway PostgreSQL)
DATABASE_URL=postgresql://user:password@host:port/database
ConnectionStrings__DefaultConnection=Host=host;Database=database;Username=user;Password=password

# JWT Configuration
Jwt__SecretKey=your_super_secure_jwt_key_here_at_least_32_characters
Jwt__Issuer=QuestionnaireSurvey
Jwt__Audience=QuestionnaireSurvey

# Google OAuth Configuration
GOOGLE_CLIENT_ID=your_google_client_id_here
GOOGLE_CLIENT_SECRET=your_google_client_secret_here

# Admin Configuration
AdminEmails=your_admin_email@gmail.com,another_admin@gmail.com

# Railway specific
PORT=8080
ASPNETCORE_ENVIRONMENT=Production
ASPNETCORE_URLS=http://+:8080

# CORS - Frontend URL
FRONTEND_URL=https://your-frontend-service.railway.app
